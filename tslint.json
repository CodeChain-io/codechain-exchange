{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "array-type": false
  },
  "jsRules": {
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "server/**/*.json",
      "server/**/*.test.ts"
    ]
  }
}
